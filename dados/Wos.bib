@article{ WOS:000408200200003,
Author = {Cush, Peter and Macken-Walsh, Aine},
Title = {{THE POTENTIAL FOR JOINT FARMING VENTURES IN IRISH AGRICULTURE: A
   SOCIOLOGICAL REVIEW}},
