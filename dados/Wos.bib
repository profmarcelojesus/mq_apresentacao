@article{ WOS:000588458000003,
Author = {Mhlaba, P. and Yusuf, S. F. G.},
Title = {{PROSPECTS OF COMMUNITY RADIO BROADCAST AS AGRICULTURAL EXTENSION SERVICE
   DELIVERY TOOL TO SMALLHOLDER FARMERS IN SOUTH AFRICA}},
Journal = {{JOURNAL FOR NEW GENERATION SCIENCES}},
Year = {{2020}},
Volume = {{18}},
Number = {{1}},
Pages = {{31-44}},
Abstract = {{Community radio is a powerful, undisputed and widely used communication
   mass medium for information sharing to rural audiences across Africa due
   to its low cost, accessibility, popularity, participatory approach and
   ability to penetrate marginalised farmers of developing countries. As
   access to proven agricultural information is crucial to farmers, this
   study examined how Community Radio Broadcast (CRB) could be used to
   complement agricultural extension service delivery in South Africa
   targeting smallholder farmers. A total of 167 community radio stations
   (CRS) with usable data were analysed from South Africa. Based on
   programming format, language of communication used, listenership and
   target audience by CRS, a questionnaire was prepared for station
   managers. All public domain CRS websites were accessed to analyse the
   programming. The results indicated that amongst the sampled CRS, local
   languages are popular for programme presentations with English being
   used by more than 50\% of the CRS. The target audience was varied and
   diversified; however, only 2.85\% out of nine provinces are targeting
   farmers. Listenership distribution is large, cutting across different
   age groups with an average listenership of 964 000/province/day.
   However, the number of agricultural programmes aired on South African
   CRS is very limited. In an attempt to use CRB as an avenue for reaching
   the smallholder farmers, the study suggests an Agricultural Radio
   Information System (ARIS) model as a strategy to complement agricultural
   extension advisory services of South Africa.}},
