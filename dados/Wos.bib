@article{ WOS:000471604500008,
Author = {Wakamoto, Keiko},
Title = {{The Reproduction Mechanism of Branded Wagyu Meat: Maesawa Beef, Oshu
   City, Iwate Prefecture}},
Journal = {{JOURNAL OF GEOGRAPHY-CHIGAKU ZASSHI}},
