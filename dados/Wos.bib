@article{ WOS:000636778200026,
Author = {Dharmawan, Rama and Sumiarto, Bambang and Wibawa, Hendra and Pramastuti,
   Ira and Sutiyarmo, Sutiyarmo and Poermadjaja, Bagoes},
Title = {{Contact rate and risk factors of classical swine fever disease in
